
<h2>Listing Users</h2>
<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Designation</th>
      <th>Description</th>
    </tr>
  </thead>

  <tbody>
    <% @project_maps_selected.each do |project_map| %>
      <tr>
		<% profile = Profile.find_by user_id:project_map.user_id %>
		<td><%= link_to  profile.name ,  profile_path(profile.id) %></td>
        <td><%= profile.designation.name %></td>
        <td><%= project_map.description %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
